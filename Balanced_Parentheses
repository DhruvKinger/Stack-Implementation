#include <bits/stdc++.h>
#include <iostream>
using namespace std;

 bool Parantheis_balanced(string expr)
   {
       
    std::stack<char> s ;
    char x;
    
    for(int i=0;i<expr.length();i++)
    {
        if(expr[i]=='['||expr[i]=='{'||expr[i]=='(')
        {
            s.push(expr[i]);
            continue;
        }
        if(s.empty()) 
           return false; 
           
         else
         {
             x=s.top();
           if(expr[i]==x)
           {
               s.pop();
             }
             else
             {
               return false;
               break;
             }
         }
           
   }
     return (s.empty());
       
   }
int main()
{
    string expr="{}";
if(Parantheis_balanced(expr)==1)
cout<<"Balanced";
else 
cout<<"Not balanced";
    

     return 0;
}
